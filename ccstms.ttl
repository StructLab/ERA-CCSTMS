###  http://data.europa.eu/949/signalOrientation
era:signalOrientation rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf era:signalObjParameter ;
                      rdf:type owl:FunctionalProperty ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( era:CommonCharacteristicsSubset
                                                  era:Signal
                                                )
                                  ] ;
                      rdfs:range skos:Concept ;
                      era:XMLName "SIG_LocDir" ;
                      era:applicable "Y/N/NYA" ;
                      era:inSkosConceptScheme <http://data.europa.eu/949/concepts/orientations/OrientationDirections> ;
                      era:legalDeadline "12 months after publication of Article 7 Guide"@en ;
                      era:rinfIndex "1.1.1.3.14.3" ,
                                    "1.2.1.0.8.3" ;
                      era:shaclShapeValidationRule <http://data.europa.eu/949/shapes/SignalOrientation> ,
                                                   <http://data.europa.eu/949/shapes/SignalOrientationSKOS> ;
                      era:tsiOPEAppendixD2Index "2.2.3" ,
                                                "2.3.3" ;
                      dcterms:created "2023-03-14"^^xsd:date ;
                      dcterms:modified "2024-04-18"^^xsd:date ;
                      dcterms:relation era:signalId ;
                      dcterms:source <http://data.europa.eu/eli/reg_impl/2019/773/oj> ;
                      rdfs:comment "Relative position to the line of reference, given in km and indication if the signal refers to normal or opposite track direction"@en ;
                      rdfs:isDefinedBy <http://data.europa.eu/949/> ;
                      rdfs:label "Signal orientation"@en ;
                      vs:term_status "unstable" ;
                      skos:example """@prefix era: <http://era.europa.eu/949/> .
@prefix era-orient: <http://data.europa.eu/949/concepts/orientations/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

era:signal1 a era:Signal ;
    #signal's position
    era:netReference [
        a era:NetPointReference ;
        era:hasLrsCoordinate [
            a era:LinearPositioningSystemCoordinate ;
            era:kmPost era:kp5 ;
            era:offsetFromKilometricPost 231;
            rdfs:label \"5+231\"@en ;
        ]
    ];
    #signal's orientation (Normal)
    era:signalOrientation era-orient:00.

era:kp5 a era:KilometricPost ;
    era:hasLRS [
        a era:LinearPositioningSystem ;
        era:lineId \"National line 1\"
    ] ;
    era:kmPostName \"km 5\" ;
    era:measuredDistance 4995 .""" ;
                      skos:scopeNote """Indication if the signal is applicable for operation on normal, opposite track direction or if it contains bidirectionally valid information (radio-based system only).

The position of the signal is given in relation to a line reference, using the era:netReference property. In general it represents the kilometric point on the main line where the signal is positioned, or a distance from an origin point, when there are no kilometric posts available on the tracks/sidings. See the example."""@en .


